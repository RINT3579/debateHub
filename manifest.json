{
  "name": "debateHub",
  "version": "0.0.5",
  "description": "GitHubPageで管理するページをChrome上から変更・プルリクエストできます",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contents.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "contextMenus"
  ],
  "options_ui": {
    "page": "config.html",
    "chrome_style": true
  },
  "manifest_version": 2
}