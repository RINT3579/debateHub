{
  "name": "Debate_Hub_",
  "version": "2.6.1",
  "description": "テスト",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "ツールメニューを選択する",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ],
  "options_page": "config.html",
  "options_ui": {
    "page": "config.html",
    "chrome_style": true,
    "open_in_tab": false
  },
  "manifest_version": 2
}